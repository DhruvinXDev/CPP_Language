#include <stdio.h>
#include <math.h>

int main() {
    // Declare the variables for height and distance
    double H, L;
    
    // Read the input values
    scanf("%lf %lf", &H, &L);
    
    // Calculate the depth of the lake using the derived formula
    double depth = sqrt(H * H - L * L);
    
    // Print the result with a precision of 6 decimal places
    printf("%.6lf\n", depth);
    
    return 0;
}
