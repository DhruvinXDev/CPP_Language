#include <stdio.h>


void main() {

    int i,n,sum=1;
    printf("Enter the number : ");
    scanf("%d",&n);
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0)
        {
            sum+=i;
        }
    }
    if(sum==n)
        printf("Perfect Number.");
    else
        printf("Not Perfect Number.");


}
