#include<stdio.h>

void main()
{
    char s[50];
    printf("Enter string : ");
    gets(s);
    for(int i=0;s[i]!='\0';i++)
    {
        if((s[i]>=65)&&s[i]<=90)
        {
            s[i]=s[i]+32;
        }
        else if((s[i]>=97)&&(s[i]<=122)){

            s[i]=s[i]-32;
        }

    }
    printf("new string is : ");
    puts(s);
}
